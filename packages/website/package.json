{
  "name": "website",
  "version": "1.11.0",
  "description": "nft.storage website",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build && next export",
    "start": "next start",
    "test": "eslint './**/*.js' && tsc --build",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@magic-ext/oauth": "^0.8.0",
    "bytes": "^3.1.0",
    "clsx": "^1.1.1",
    "countly-sdk-web": "^20.11.3",
    "filesize": "^6.1.0",
    "ipfs-car": "0.4.3",
    "isomorphic-form-data": "^2.0.0",
    "magic-sdk": "^4.2.1",
    "nft.storage": "^3.3.0",
    "querystring-browser": "^1.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-if": "^4.0.1",
    "react-query": "^3.13.9",
    "swagger-ui-react": "^3.45.0",
    "tachyons": "^4.12.0"
  },
  "devDependencies": {
    "@sentry/nextjs": "^6.3.5",
    "@types/bytes": "^3.1.1",
    "@types/react": "^17.0.4",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "next": "^11.0.1",
    "typescript": "4.3.5"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "next",
      "next/core-web-vitals",
      "prettier"
    ],
    "env": {
      "es2020": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "args": "all",
          "argsIgnorePattern": "_",
          "varsIgnorePattern": "_"
        }
      ],
      "react/jsx-filename-extension": [
        "warn",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/display-name": "warn",
      "@next/next/no-img-element": "off"
    }
  },
  "eslintIgnore": [
    "node_modules",
    ".next",
    "out"
  ]
}
